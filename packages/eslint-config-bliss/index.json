{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended",
    "prettier/react",
    "plugin:jest/recommended",
    "plugin:promise/recommended",
    "plugin:markdown/recommended"
  ],
  "env": {
    "node": true,
    "jest": true
  },
  "plugins": [
    "compat",
    "prettier",
    "promise",
    "jest"
  ],
  "rules": {
    "import/no-cycle": "off"
  }
}
